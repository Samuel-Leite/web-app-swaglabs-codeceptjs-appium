{
  "name": "mobile-automation",
  "version": "1.0.0",
  "description": "Projeto de testes automatizados",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "precommit": "git add . ",
    "commit": "git cz && node .husky/push.js",
    "test": "npx codeceptjs run --steps",
    "web": "npx codeceptjs run ./tests/e2e/*_test.js && allure serve output",
    "tag": "npx codeceptjs run ./tests/e2e/*_test.js --grep",
    "lint": "eslint src --max-warnings=0"
  },
  "lint-staged": {
    "src/**/*": [
      "npx lint --fix"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "codeceptjs": "^3.4.1",
    "dotenv": "^16.3.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "node-random-name": "^1.0.1",
    "prettier": "^2.8.7",
    "random-number": "^0.0.9",
    "webdriverio": "^8.6.9"
  },
  "devDependencies": {
    "@codeceptjs/allure-legacy": "^1.0.2",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0"
  }
}
